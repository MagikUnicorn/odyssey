{
  "schema_version": 1,

  "quilt_loader": {

    "group": "net.sydokiddo",
    "id": "odyssey",
    "version": "${version}",

    "metadata": {
      "name": "Odyssey",
      "description": "A collection of non-specific features and items that expand upon the base game and its content!",
      "contributors": {
        "Sydokiddo": "Owner"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/odyssey",
        "issues": "https://github.com/Sydokiddo/odyssey/issues",
        "sources": "https://github.com/Sydokiddo/odyssey"
      },
      "license": "MIT",
      "icon": "assets/odyssey/icon.png"
    },

    "intermediate_mappings": "net.fabricmc:intermediary",

    "entrypoints": {
      "init": "net.sydokiddo.odyssey.Odyssey",
      "client": "net.sydokiddo.odyssey.client.OdysseyClient",
      "modmenu": "net.sydokiddo.odyssey.misc.config.ModMenuCompatibility"
    },

    "depends": [
